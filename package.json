    "files": [
        "lib"
    ],
    "scripts": {
        "test": "tsc && nyc mocha "
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/enncy/json-puppeteer.git"
    },
    "keywords": [
        "puppeteer",
        "json",
        "headless-chrome",
        "nodejs",
        "typescript"
    ],
    "author": "enncy",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/enncy/json-puppeteer/issues"
    },
    "homepage": "https://github.com/enncy/json-puppeteer#readme",
    "dependencies": {
        "axios": "^0.24.0",
        "denque": "^2.0.1",
        "jsonsep": "link:",
        "puppeteer-core": "^13.0.1",
        "reflect-metadata": "^0.1.13"
    },
    "devDependencies": {
        "@types/chai": "^4.3.0",
        "@types/mocha": "^9.0.0",
        "@types/node": "^17.0.8",
        "chai": "^4.3.4",
        "mocha": "^9.1.3",
        "nyc": "^15.1.0",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.4"
    },
    "nyc": {
        "include": [
            "src/**/*.ts"
        ],
